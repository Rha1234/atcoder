    "N, M = map(int, input().split())\n",
    "A = list(map(int, input().split()))\n",
    "\n",
    "total = sum(A)\n",
    "count = 0\n",
    "\n",
    "for a in A:\n",
    "    if a >= total/(4*M):\n",
    "        count += 1\n",
    "\n",
    "print('Yes' if count >= M else 'No')"
