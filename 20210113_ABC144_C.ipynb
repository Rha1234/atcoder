    "import math\n",
    "N = int(input())\n",
    "max_n = 1\n",
    "\n",
    "for i in range(1, round(math.sqrt(N))+1):\n",
    "    if N % i == 0:\n",
    "        max_n = i\n",
    "print(max_n - 1 + (N//max_n) - 1)"
