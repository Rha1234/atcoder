   "source": [
    "#入力を受け取る\n",
    "S=input()\n",
    "\n",
    "#回文の前半・後半を作る\n",
    "length=len(S)//2\n",
    "\n",
    "A=S[:length]\n",
    "B=S[::-1][:length]\n",
    "\n",
    "#回文の前半・後半を比較して、不一致数を求める\n",
    "change_nums=0\n",
    "\n",
    "for a,b in zip(A,B):\n",
    "    if a!=b:\n",
    "        change_nums +=1\n",
    "\n",
    "#出力する\n",
    "print(change_nums)"
   ]
